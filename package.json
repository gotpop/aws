{
	"name": "aws",
	"version": "0.1.0",
	"private": true,
	"description": "AWS Lambda services for gotpop platform",
	"workspaces": [
		"apps/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"deploy": "turbo run deploy",
		"format:check": "turbo run format:check",
		"format:fix": "turbo run format:fix",
		"lint": "turbo run lint",
		"export:templates": "yarn workspace email-preview export:templates",
		"type-check": "turbo run type-check"
	},
	"engines": {
		"node": ">=22.20.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.6",
		"turbo": "^2.0.0",
		"typescript": "^5.7.3"
	},
	"prettier": {
		"semi": false,
		"singleQuote": false,
		"trailingComma": "es5",
		"tabWidth": 2,
		"printWidth": 80,
		"arrowParens": "always"
	}
}
